{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"files": [
			"!tsc_output"
		],
		"require": [
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"bin": {
		"bootleg": "./bin/run"
	},
	"bugs": {
		"url": "https://github.com/pizzafox/bootleg/issues"
	},
	"dependencies": {
		"@oclif/command": "1.8.0",
		"convert": "1.1.1",
		"got": "11.8.0"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "1.0.1",
		"@tsconfig/node14": "1.0.0",
		"@types/node": "14.14.10",
		"ava": "3.13.0",
		"eslint-plugin-prettier": "3.1.4",
		"file-type": "16.0.1",
		"nyc": "15.1.0",
		"prettier": "2.2.1",
		"source-map-support": "0.5.19",
		"ts-node": "9.0.0",
		"type-fest": "0.20.2",
		"typedoc": "0.19.2",
		"typescript": "4.0.3",
		"xo": "0.35.0"
	},
	"files": [
		"./bin"
	],
	"license": "Apache-2.0",
	"main": "./tsc_output/index.js",
	"name": "bootleg",
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.ts"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"oclif": {
		"bin": "bootleg"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pizzafox/bootleg.git"
	},
	"scripts": {
		"build": "tsc",
		"docs": "typedoc --out docs_out --mode file --exclude \"src/*/*.test.*\"",
		"lint": "xo",
		"prebuild": "rm -rf tsc_output",
		"pretest": "rm -rf coverage .nyc_output test/downloads",
		"style": "prettier --ignore-path .gitignore --check .",
		"test": "nyc ava"
	},
	"version": "1.0.0",
	"xo": {
		"prettier": true
	}
}
