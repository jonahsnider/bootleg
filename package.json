{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"files": [
			"!tsc_output"
		],
		"require": [
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"bin": {
		"bootleg": "./bin/run"
	},
	"bugs": {
		"url": "https://github.com/jonahsnider/bootleg/issues"
	},
	"dependencies": {
		"@ltd/j-toml": "1.12.2",
		"@oclif/command": "1.8.0",
		"cli-ux": "5.6.3",
		"convert": "2.1.1",
		"got": "11.8.2",
		"p-queue": "7.1.0",
		"tough-cookie": "4.0.0"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "1.0.1",
		"@tsconfig/node14": "1.0.1",
		"@types/node": "14.17.5",
		"@types/tough-cookie": "4.0.1",
		"ava": "3.15.0",
		"eslint-plugin-prettier": "3.4.0",
		"file-type": "16.5.1",
		"nyc": "15.1.0",
		"prettier": "2.3.2",
		"source-map-support": "0.5.19",
		"ts-node": "10.1.0",
		"type-fest": "1.2.2",
		"typedoc": "0.21.4",
		"typescript": "4.3.5",
		"xo": "0.42.0"
	},
	"files": [
		"./bin"
	],
	"license": "Apache-2.0",
	"main": "./tsc_output/index.js",
	"name": "bootleg",
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.ts"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"oclif": {
		"bin": "bootleg"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/bootleg.git"
	},
	"scripts": {
		"build": "tsc",
		"docs": "typedoc --out docs_out src/index.ts",
		"lint": "xo",
		"prebuild": "rm -rf tsc_output",
		"pretest": "rm -rf coverage .nyc_output test/downloads",
		"style": "prettier --ignore-path .gitignore --check .",
		"test": "nyc ava"
	},
	"version": "1.0.0",
	"xo": {
		"prettier": true,
		"rules": {
			"unicorn/prefer-node-protocol": "off",
			"unicorn/prefer-module": "off"
		}
	}
}
